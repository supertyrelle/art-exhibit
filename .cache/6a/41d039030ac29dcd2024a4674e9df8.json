// rows\n.row-sb-c {\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n    align-items: center;\n    flex-wrap: wrap;\n}\n\n.row-sb-fs {\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n    align-items: flex-start;\n    flex-wrap: wrap;\n}\n.row-sa {\n    display: flex;\n    flex-direction: row;\n    justify-content: space-around;\n    flex-wrap: wrap;\n}\n\n.row-sa-c {\n    display: flex;\n    flex-direction: row;\n    justify-content: space-around;\n    align-items: center;\n    flex-wrap: wrap;\n}\n\n.row-c-c {\n    display: flex;\n    flex-direction: row;\n    justify-content: center;\n    align-items: center;\n    flex-wrap: wrap;\n}\n\n// columns\n.col {\n    display: flex;\n    flex-direction: column;\n    align-items: flex-start;\n    flex-wrap: wrap;\n}\n\n.col-c {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    flex-wrap: wrap;\n}\n\n.col-c {\n    display: flex;\n    flex-direction: column;\n    // justify-content: space-around;\n    align-items: center;\n    flex-wrap: wrap;\n    align-content: space-between;\n}\n\n.col-fs {\n    display: flex;\n    flex-direction: column;\n    align-items: flex-start;\n    flex-wrap: wrap;\n}\n\n// element styles\nbutton {\n    padding: 1rem 2rem;\n    border-radius: 100px;\n    font-size: 18pt;\n    border: none;\n}","scss/style.scss":"@import 'tech';\n@import 'framework';\n\n// header\nheader {\n    width: 95vw;\n    margin: auto;\n    max-width: 1400px;\n\n    .logo {\n        font-size: 40pt;\n        position: relative;\n\n        &::after {\n            content: 'Art Exhibit';\n            position: absolute;\n            right: 0px;\n            bottom: -0.5em;\n            display: block;\n            font-size: 12pt;\n        }\n\n    }\n\n    nav {\n        font-size: 14pt;\n\n        a {\n            color: black;\n            text-decoration: none;\n\n            &+a {\n                margin-left: 2rem;\n            }\n        }\n    }\n}\n\n// main\nmain {\n    min-height: 75vh;\n    max-width: 1400px;\n    margin: auto;\n\n    .call-to-action {\n        // padding-top: 2rem;\n        position: relative;\n        height: 75vh;\n\n        .cta-content {\n            width: 35%;\n\n            h1 {\n                font-size: 38pt;\n                line-height: 50pt;\n                margin-bottom: 1rem;\n                // text-align: justify;\n            }\n\n            p {\n                margin-top: 1em;\n            }\n\n            button {\n                margin-top: 1em;\n                width: 100%;\n                background: rgba($primary, 65%);\n                color: $black;\n                font-weight: 100;\n                transition: background ease 230ms;\n\n                &:hover {\n                    background: rgba($primary, 85%);\n                    cursor: pointer;\n                }\n            }\n        }\n\n        .cta-ill {\n            display: flex;\n\n            img {\n                &+img {\n                    padding-left: 5px;\n                }\n\n                &:nth-child(4) {\n                    padding-left: 0px;\n                }\n            }\n        }\n    }\n\n    .ex-stats {\n        height: auto;\n\n        .ex-stats-art {\n            position: relative;\n            width: 100%;\n            max-width: 700px;\n            top: 0px;\n            left: 0px;\n            display: inline-block;\n            height: 325vh;\n\n            img {\n                position: sticky;\n                top: 4rem;\n                left: 0px;\n                display: inline-block;\n                width: 100%;\n            }\n        }\n\n        .ex-stats-content {\n            align-self: stretch;\n            flex: 2;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            justify-content: flex-start;\n            align-content: center;\n\n            .fact {\n                align-self: stretch;\n                padding-top: 25vh;\n                padding-bottom: 50vh;\n\n                h1 {\n                    font-size: 128pt;\n                    line-height: 150pt;\n                    color: $black;\n                }\n\n                h2 {\n                    font-weight: 100;\n                    font-size: 40pt;\n                }\n\n                ul,\n                li {\n                    list-style-type: none;\n                    list-style-position: inside;\n                    margin: 0;\n                    padding: 0;\n                }\n\n                li {\n                    padding: 1em 1em;\n                    font-size: 16pt;\n                    background: rgba($black, 2%);\n                    margin: 0.5em 0;\n                    display: block;\n                }\n\n                &:nth-child(2) {\n                    padding-bottom: 35vh;\n                }\n\n                &:last-child {\n                    padding-bottom: 25vh;\n\n                }\n            }\n        }\n    }\n}\n\n// footer\nfooter {\n    margin-top: 8em;\n    width: 100%;\n    background: rgba($black, 15%);\n\n    .logo {\n        font-size: 25pt;\n        position: relative;\n\n        &::after {\n            content: 'Art Exhibit';\n            position: absolute;\n            right: 0px;\n            bottom: -0.5em;\n            display: block;\n            font-size: 8pt;\n        }\n    }\n\n    .footer-content,\n    .legal {\n        width: 100%;\n        padding: 0 2.5rem;\n\n    }\n\n    .footer-content {\n        padding: 1rem 0rem;\n\n        .quick-links {\n            h5 {\n                text-transform: uppercase;\n                color: rgba($black, 50%);\n                font-size: 14pt;\n                margin-bottom: 0.5em;\n            }\n\n            a {\n                // margin-left:0.5em;\n                text-decoration: none;\n\n                &+a {\n                    marg